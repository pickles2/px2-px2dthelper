{
    "name": "pickles2/px2-px2dthelper",
    "description": "Pickles 2 用のプラグインです。Pickles 2 アプリケーション と連携させるためのAPIを提供します。",
    "keywords": ["pickles2"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tomoya Koyanagi",
            "email": "tomk79@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts":{
        "test": [
            "php ./vendor/phpunit/phpunit/phpunit"
        ],
        "documentation": [
            "php ./vendor/phpdocumentor/phpdocumentor/bin/phpdoc --title \"pickles2/px2-px2dthelper@2.0.19 API Document\" -d \"./php/\" -t \"./doc/\""
        ]
    } ,
    "repositories": [
    ],
    "require": {
        "php": ">=5.4.0",
        "tomk79/php-excel2html": "^0.0.8||~0.1",
        "michelf/php-markdown": "~1.6",
        "leafo/scssphp": ">=0.7",
        "pickles2/lib-px2-contents-editor": "~2.0",
        "pickles2/lib-px2-module-editor": "~0.2",
        "pickles2/lib-px2-theme-editor": "~0.0",
        "broccoli-html-editor/broccoli-html-editor": "~0.4"
    },
    "suggest": {
        "scssphp/scssphp": "This is the successor package to \"leafo/scssphp\"."
    },
    "require-dev": {
        "broccoli-html-editor/broccoli-module-fess": "*",
        "broccoli-html-editor/broccoli-module-plain-html-elements": "*",
        "pickles2/px-fw-2.x": "~2.0" ,
        "pickles2/px2-sitemapexcel": "~2.0",
        "pickles2/px2-multitheme": "~2.0",
        "tomk79/filesystem": "~1.0.0",
        "phpunit/phpunit": "^4.8.0",
        "phpdocumentor/phpdocumentor": "^2.9.0"
    },
    "autoload": {
        "psr-4": {
            "tomk79\\pickles2\\px2dthelper\\": [
                "php"
            ]
        }
    }
}
